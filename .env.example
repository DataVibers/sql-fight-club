# Your OpenAI API key (required)
OPENAI_API_KEY=your-api-key-here

# Model to use for the SQL agents
OPENAI_MODEL=gpt-4.1-mini

# Default number of rounds in a fight
FIGHT_ROUNDS_DEFAULT=6

# Max rows to show in preview tables
MAX_ROWS_PREVIEW=20

# DuckDB database path
# Use :memory: for in-memory DB, or a file like duckdb/sql_fight_club.db
DUCKDB_DB_PATH=:memory:
